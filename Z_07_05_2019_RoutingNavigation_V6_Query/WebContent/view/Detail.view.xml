<mvc:View controllerName="sap.ui.opensap.controller.Detail"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>detailPageTitle}" showNavButton="true"
		navButtonPress=".onNavBack">
		<content>
		<List items="{AGNI>/oData}">	
				<StandardListItem			
					title = "{AGNI>ProductName}"
					info = "{AGNI>Quantity}">
				</StandardListItem>
			
		</List>
		</content>
	</Page>
</mvc:View>


