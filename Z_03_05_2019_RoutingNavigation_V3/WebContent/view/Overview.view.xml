<mvc:View
		controllerName="sap.ui.opensap.controller.App"
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>homePageTitle}">
		<headerContent>
			<Button
					icon="sap-icon://hello-world"
					press=".onOuvertDialog"/>
		</headerContent>
		<content>
			<mvc:XMLView viewName="sap.ui.opensap.view.app_Main"/>
			<mvc:XMLView viewName="sap.ui.opensap.view.InvoiceList"/>
		</content>
	</Page>
</mvc:View>
